   "id": "a109c5cf",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fatal - injuries that resulted in death\n",
    "# Incapacitating - serious injuries require immediate medical attention\n",
    "\n",
    "## BICYCLE_DEATH_BY_AUTO_COUNT\n",
    "# Filter the DataFrame for cyclist fatalities\n",
    "fatal_injury_condition_2021 = df_crash_2021['Injury 04'] > 0\n",
    "\n",
    "bicycle_condition_2021 = df_crash_2021['Crash Type'] == 'Bicycle/Motorized Bicycle'\n",
    "\n",
    "vehicle_1_condition_2021 = df_crash_2021['TU-1 Type'].isin(['SUV', 'Passenger Car/Passenger Van', \n",
    "       'Pickup Truck/Utility Van',\n",
    "       'Other Vehicle Type (Describe in Narrative)', 'Motorcycle',\n",
    "       'Medium/Heavy Trucks, GVWR/GCWR 10,001 or Over', 'Transit Bus',\n",
    "       'School Bus (all school buses)', 'Motor Home',\n",
    "       'Medium/Heavy Trucks, GVWR/GCWR 16,001 or over',\n",
    "       'Non-School Bus (nine occupants or more including driver) in commerce',\n",
    "       'Working Vehicle/Equipment', 'Autocycle',\n",
    "       'Off Highway Vehicle/ATV', 'Low Speed Vehicle',\n",
    "       'Passenger Car/Passenger Van with Trailer - RET', 'Heavy Train'])\n",
    "\n",
    "vehicle_2_condition_2021 = df_crash_2021['TU-2 Type'].isin(['SUV', 'Passenger Car/Passenger Van',\n",
    "       'Pickup Truck/Utility Van', 'Motorcycle',\n",
    "       'Non-School Bus (nine occupants or more including driver) in commerce',\n",
    "       'Other Vehicle Type (Describe in Narrative)',\n",
    "       'Medium/Heavy Trucks, GVWR/GCWR 10,001 or Over', 'Transit Bus',\n",
    "       'Motor Home', 'Medium/Heavy Trucks, GVWR/GCWR 16,001 or over',\n",
    "       'School Bus (all school buses)', 'Working Vehicle/Equipment',\n",
    "       'Light Rail', 'Heavy Train',\n",
    "       'Off Highway Vehicle/ATV', 'Low Speed Vehicle', 'Autocycle',\n",
    "       'Pickup Truck/Utility Van with Trailer - RET'])\n",
    "\n",
    "# Corrected variable names in the next line\n",
    "cyclist_fatalities_2021 = df_crash_2021[fatal_injury_condition_2021 & bicycle_condition_2021 & (vehicle_1_condition_2021 | vehicle_2_condition_2021)]\n",
    "\n",
    "# Group the filtered DataFrame by 'CITY' and 'COUNTY' and calculate the count for each city\n",
    "bicycle_death_counts_2021 = cyclist_fatalities_2021.groupby(['City', 'County']).size().reset_index(name='BICYCLE_DEATH_BY_AUTO_COUNT')\n",
    "\n",
    "# Merge the bicycle_death_counts DataFrame into df_crash_2021 on 'CITY' and 'COUNTY'\n",
    "df_crash_2021 = df_crash_2021.merge(bicycle_death_counts_2021, on=['City', 'County'], how='left')"
   ]
  },
