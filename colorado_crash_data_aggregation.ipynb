   "id": "93d1cae6",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Exclude the NaN from 'CITY' and 'ACCTYPE'\n",
    "df_crash_2021 = df_crash_2021[df_crash_2021['City'].notna()]\n",
    "df_crash_2021 = df_crash_2021[df_crash_2021['Crash Type'].notna()]"
   ]
  },
  {
   "cell_type": "code",
